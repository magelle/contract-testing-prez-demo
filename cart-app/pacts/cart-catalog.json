{
  "consumer": {
    "name": "cart"
  },
  "provider": {
    "name": "catalog"
  },
  "interactions": [
    {
      "description": "a request to GET a Product",
      "request": {
        "method": "GET",
        "path": "/products/1"
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "name": "MyProduct",
          "id": 1,
          "price": 12
        },
        "matchingRules": {
          "$.body": {
            "match": "type"
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}